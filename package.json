  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/stream-to-array": "^2.3.1",
    "eslint": "^8.48.0",
    "prettier": "3.0.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "typescript-require": "^0.3.0"
